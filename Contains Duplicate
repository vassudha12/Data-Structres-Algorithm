class Solution {
    public boolean containsDuplicate(int[] nums) {
        Arrays.sort(nums);
        int l=nums.length;
        // int i=0;
        // int j=1;
        boolean flag=false;
        for(int i=0;i<l-1;i++){
            if(nums[i]==nums[i+1]){
                flag=true;
                break;
            }
        }
        return flag;
    }
}
